            "program": "${workspaceFolder}/project/dist/rdp_nodefetch.js",
            "args": ["--symbol","LSEG.L"],
            "runtimeArgs": ["--experimental-fetch"],
            "console": "internalConsole",
            "stopAtEntry": false,
            "preLaunchTask": "Compile"
        }
    ]
}